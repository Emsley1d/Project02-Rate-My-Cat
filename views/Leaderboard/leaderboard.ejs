<% title = "Leaderboard!" %>

<nav>
  <div class="flex-container" style="overflow-y: scroll; width: 1000px">
    <div class="main-nav pad">

      <% let order = cats.sort(function(b, a) { return a.rating - b.rating; });
cats.forEach(function(cat) { %>
  <% console.log(order) %>

      <div class="w3-card-4" style="width: 50%">
        <a href="/cat/detail?id=<%= cat._id %>">
          <img id="catImage"
            src="..<%= cat.image %>"
            alt="<%= cat.name %>"
            style="width: 100%"
          />
        </a>
        <div class="catFooter">
          <h4><%= cat.name %> - rated: <%= cat.rating %></h4>
        </div>
      </div>
      <% }) %>
    </div>
  </div>
</nav>
