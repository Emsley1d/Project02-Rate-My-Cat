<h2> <%= cat.catname %> </h2>

<!-- IMAGE PLACE HOLDER -->
<div>
    Image: <%= cat.image %>

</div>
<!-- IMAGE PLACE HOLDER -->

<!-- RATINGS SLIDER -->
<form action="/action_page.php" method="get">
    <label for="vol">Rating (1 - 10):</label>
    <input type="range" id="vol" name="vol" min="1" max="10" step="1" oninput="this.nextElementSibling.value = this.value"/>
  <output></output> &nbsp;
    <input type="submit" value="Submit">

<div>
    Age: <%= cat.age %>
</div>

<div>
    Breed: <%= cat.breed %>
</div>

<div>
    Location: <%= cat.location %>
</div>

<div>
    Likes: <%= cat.likes %>
</div>

<div>
    Dislikes: <%= cat.dislikes %>
</div>

<div>
    CreatedAt: <%= moment(cat.createdAt).fromNow() %>
</div>

<div>
   UpdatedAt: <%= moment(cat.updatedAt).fromNow() %>
</div>

<br>

<% cat.user.forEach(function(user) { %>

<div> Owner: <%= user.name %></div>
<div> CreatedAt: <%= moment(user.createdAt).fromNow() %></div>
<hr>

<% }) %>
