<% title = "Locations" %>

<nav>
  <div class="flex-container">
    <div class="main-nav pad">
      <a href="/location/index" class="btn btn-outline-primary">Locations</a>

      <table class="table table-striped">
        <tr>
          <th>Locations</th>
        </tr>

        <% users.forEach(function(user){ %>
        <tr>
          <td><h3><%= user.location %> </h3></td>
          <td><%= user.cat.name %></td>
        </tr>
        <% }) %>
      </table>
    </div>
  </div>
</nav>
