<% title = "Ailurophiles (fancyspeak for Cat Lover)" %>

<nav>
  <div class="flex-container">
    <div class="main-nav pad">
      <table class="table table-striped">
        <tr>
          <th>First Name</th>
          <th>Last Name</th>
          <th>Location</th>
          <th>Bio</th>
          <th>Created At</th>
        </tr>

        <% users.forEach(function(user){ %>
        <tr>
          <td>
            <a href="/user/detail?id=<%= user._id %>"><%= user.firstName %></a>
          </td>
          <td>
            <a href="/user/detail?id=<%= user._id %>"><%= user.lastName %></a>
          </td>
          <td><%= user.location %></td>
          <td><%= user.bio %></td>
          <td><%= moment(user.createdAt).fromNow() %></td>
          <% }) %>
        </tr>

      </table>
    </div>
  </div>
</nav>
