<% title = "User Index" %>
    
<nav> 
     <div class="flex-container">
        <div class="main-nav pad">
            <table class="table table-striped">
                <tr>
                    <th> First Name</th>
                    <th> Last Name </th>
                    <th>Email Address</th>
                    <th> Location </th>
                    <th> Bio </th>
                    <th>Created At</th>
                    <th>Edit</th>
                    <th>Delete</th>
                </tr>
            
            <% users.forEach(function(user){   %>  
            <tr>
                <td> <a href="/user/detail?id=<%= user._id %>"><%= user.firstName %></a> </td>
                <td> <a href="/user/detail?id=<%= user._id %>"><%= user.lastName %></a> </td>
                <td> <%= user.emailAddress %></td>
                <td> <%= user.location %></td>
                <td> <%= user.bio %></td>
                <td> <%= moment(user.createdAt).fromNow() %></td>
                <td><a href="/user/edit?id=<%= user._id %>">Edit</a></td>
                <td><a href="/user/delete?id=<%= user._id %>">Delete</a></td>
            </tr>
            <% }) %>
            
            </table>
        </div>
    </div>

</div>  
</nav>
   